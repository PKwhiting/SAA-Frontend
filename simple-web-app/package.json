{
  "name": "simple-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli releases files `git rev-parse --short HEAD` upload-sourcemaps dist/js --url-prefix '~/js' --ext js.map:js.map --project simplicars"
  },
  "dependencies": {
    "@sentry/vue": "^7.77.0",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "jquery": "^3.7.1",
    "popper.js": "^1.16.1",
    "vue": "^3.2.13",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.21.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "vue-loader": "^17.3.0",
    "webpack": "^5.89.0"
  }
}
